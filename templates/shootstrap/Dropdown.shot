def new(id=None,links=[]):
	if id:
		.dropdown
			button.btn.dropdown-toggle.sr-only type="button" id="[[ id ]]" data-toggle="dropdown"
				: Dropdown
				span.caret
			ul.dropdown-menu role="menu" aria-labelledby="[[ id ]]"
				for link in links:
					li role="presentation"
						link role="menuitem" tabindex="-1" to="[[ link.url if link.url else (link.name|lower|replace(' ','_')) ]]": [[ link.name ]]
